[ignore]
.*coverage/.*
.*dist/.*
.*node_modules/.*
.*test/.*
.*\.config\.js
.*\.conf\.js
.*deploy\-to\-wikipedia\.js

.*\.json

[lints]
untyped-import=off
unclear-type=off

[options]
all=true
module.name_mapper='^react\-bootstrap\/.*' -> 'react-bootstrap'
module.name_mapper='^semantic\-ui\-css\/.*' -> 'semantic-ui-css'

module.name_mapper='^caches' ->'<PROJECT_ROOT>/src/caches'
module.name_mapper='^components' ->'<PROJECT_ROOT>/src/components'
module.name_mapper='^core' ->'<PROJECT_ROOT>/src/core'
module.name_mapper='^editors' ->'<PROJECT_ROOT>/src/editors'
module.name_mapper='^enhancements' ->'<PROJECT_ROOT>/src/enhancements'
module.name_mapper='^model' ->'<PROJECT_ROOT>/src/model'
module.name_mapper='^settings' ->'<PROJECT_ROOT>/src/settings'
module.name_mapper='^sources' ->'<PROJECT_ROOT>/src/sources'
module.name_mapper='^utils' ->'<PROJECT_ROOT>/src/utils'
module.name_mapper='^wrappers' ->'<PROJECT_ROOT>/src/wrappers'
